{
  "name": "necesitomasreviews",
  "version": "1.0.0",
  "description": "Plataforma de tarjetas NFC para reviews de Google",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server_fixed.js",
    "start:original": "node dist/server.js",
    "build": "tsc",
    "dev": "ts-node --transpile-only src/server.ts",
    "dev:watch": "nodemon --exec ts-node --transpile-only src/server.ts",
    "dev:fixed": "nodemon dist/server_fixed.js",
    "client": "cd client && npm run dev -- --port 5300",
    "client:build": "cd client && npm run build",
    "client:install": "cd client && npm install",
    "dev:full": "concurrently \"npm run dev:watch\" \"npm run client\"",
    "dev:full:fixed": "concurrently \"npm run dev:fixed\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nfc",
    "reviews",
    "google"
  ],
  "author": "NecesitoMasReviews",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "concurrently": "^8.2.2",
    "connect-mongodb-session": "^5.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-useragent": "^1.0.15",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "qrcode.react": "^4.2.0",
    "react-qr-code": "^2.0.15",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.17.10",
    "@types/express-useragent": "^1.0.5",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.5",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/validator": "^13.11.7",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}